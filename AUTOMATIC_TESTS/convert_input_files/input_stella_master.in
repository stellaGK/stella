   
!===============================================================================
!                               Magnetic Geometry                               
!===============================================================================
! Define geometry parameters
! Depending on <geo_option>, different extra namelists will be read: 
!    <geo_option>  = ['local', 'default', 'miller'] uses the <millergeo_parameters> namelist 
!    <geo_option>  = 'vmec' uses the <vmec_parameters> namelist 
!    <geo_option>  = 'zpinch' does not use any extra namelists 
!    <geo_option>  = 'input.profiles' does not use any extra namelists 
! Note that q_as_x  = <radial_variation>, from the <parameters_physics> namelist

&geo_knobs
   geo_option = 'local'
   overwrite_bmag = .false.
   overwrite_b_dot_grad_zeta = .false.
   overwrite_gds2 = .false.
   overwrite_gds21 = .false.
   overwrite_gds22 = .false.
   overwrite_gds23 = .false.
   overwrite_gds24 = .false.
   overwrite_gbdrift = .false.
   overwrite_cvdrift = .false.
   overwrite_gbdrift0 = .false.
   set_bmag_const = .false.
   q_as_x = .false.
   geo_file = 'input.geometry'
/

! Define parameters related to VMEC geometry
&vmec_parameters
  vmec_filename = 'wout*.nc'
  alpha0 = 0.0
  zeta_center = 0.0
  nfield_periods = -1.0
  torflux = 0.6354167d+0
  surface_option = 0
  verbose = .true.
  zgrid_scalefac = -1.0
  zgrid_refinement_factor = 1
  radial_coordinate = 'sgn(psi_t) psi_t'
  n_tolerated_test_arrays_inconsistencies = 0
  rectangular_cross_section = .false.
/

! Define parameters related to Miller geometry
&millergeo_parameters
  nzed_local = 128
  rhoc = 0.5
  rhoc0 = DEPRECATED
  rmaj = 2.77778
  rgeo = 2.77778
  qinp = 1.4
  shat = 0.796
  shift = 0.0
  kappa = 1.0
  kapprim = 0.0
  tri = 0.0
  triprim = 0.0
  betaprim = 0.0
  betadbprim = 0.0
  d2qdr2 = 0.0
  d2psidr2 = 0.0
  read_profile_variation = .false.
  write_profile_variation = .false.
  load_psi0_variables = DEPRECATED
  rhotor = DEPRECATED
  psitor_lcfs = DEPRECATED
  drhotordrho = DEPRECATED
/

! Define parameters for a z-pinch
&zpinchgeo_parameters
   betaprim = 0.0
/

!===============================================================================
!                                    Physics                                    
!===============================================================================

! Define parameters related to physics
&parameters_physics
  beta = 0.0
  vnew_ref = -1.0
  rhostar = -1.0
  irhostar = -1.0
  zeff = 1.0
  tite = 1.0
  nine = 1.0
  g_exb = 0.0
  g_exbfac = 1.0
  omprimfac = 1.0
/

&parameters_physics
  full_flux_surface = .false.
  radial_variation = .false.
  include_pressure_variation = .false.
  include_geometric_variation = .true.
  include_parallel_nonlinearity = .false.
  include_parallel_streaming = .true.
  include_mirror = .true.
  nonlinear = .false.
  adiabatic_option = 'field-line-average-term'
  const_alpha_geo = DEPRECATED
  prp_shear_enabled = .false.
  hammett_flow_shear = .true.
  suppress_zonal_interaction = .false.
  include_apar = .false.
  include_bpar = .false.
  xdriftknob = 1.0
  ydriftknob = 1.0
  wstarknob = 1.0
/

!===============================================================================
!                                Kinetic species                                
!===============================================================================

&species_knobs
  nspec = 2
  species_option = 'stella'
  read_profile_variation = .false.
  write_profile_variation = .false.
  ecoll_zeff = .false.
/

&species_parameters_1
  z = 1.0
  mass = 1.0
  dens = 1.0
  temp = 1.0
  tprim = -999.9
  fprim = -999.9
  d2ndr2 = 0.0
  d2tdr2 = 0.0
  bess_fac = 1.0
  type = "ion"
/

!===============================================================================
!                       Discretized (kx,ky,z,mu,vpa) grid                       
!===============================================================================

&kt_grids_knobs
  grid_option = 'range'
/

&kt_grids_range_parameters
  naky = 1
  nakx = 1
  aky_min = 0.0
  aky_max = 0.0
  akx_min = 0.0
  akx_max = -1.0
  theta0_min = 0.0
  theta0_max = -1.0
  phase_shift_angle = 0.
  kyspacing_option = 'default'
/

&parameters_kxky_grids_box
  nx = 1
  ny = 1
  jtwist = -1
  jtwistfac = 1.
  phase_shift_angle = 0.
  x0 = -1.0
  y0 = -1.0
  centered_in_rho = .true.
  randomize_phase_shift = .false.
  periodic_variation = .false.
/

&vpamu_grids_parameters
  nmu = 12
  nvgrid = 24
  vpa_max = 3.0
  vperp_max = 3.0
  equally_spaced_mu_grid = .false.
  conservative_wgts_vpa = .false.
/

&zgrid_parameters
  nzed = 24
  nperiod = 1
  ntubes = 1
  boundary_option = 'zero'
  zed_equal_arc = .false.
  shat_zero = 1e-05
  grad_x_grad_y_zero = 1e-05
  dkx_over_dky = -1.0
/

!===============================================================================
!                                  Diagnostics                                  
!===============================================================================

&stella_diagnostics_knobs
  nwrite = 50
  navg = 50
  nc_mult = 1
  nsave = -1
  save_for_restart = .false.
  flux_norm = .true.
  write_all = .false.
  write_phi2_vs_time = .true.
  write_apar2_vs_time = .true.
  write_bpar2_vs_time = .true. 
  write_phi_vs_kxkyz = .false.
  write_apar_vs_kxkyz = .false.
  write_bpar_vs_kxkyz = .false.
  write_phi2_vs_kxky = .false.
  write_apar2_vs_kxky = .false.
  write_bpar2_vs_kxky = .false.
  write_phi_vs_time = DEPRECATED
  write_g2_vs_vpamus = .false.
  write_g2_vs_zvpas = .false.
  write_g2_vs_zmus = .false.
  write_g2_vs_kxkyzs = .false.
  write_g2_vs_zvpamus = .false.
  write_distribution_g = .true.
  write_distribution_h = .false.
  write_distribution_f = .false.
  write_gvmus = DEPRECATED
  write_gzvs = DEPRECATED
  write_omega_vs_kxky = .false.
  write_omega_avg_vs_kxky = .false.
  write_omega = DEPRECATED
  write_kspectra = DEPRECATED
  write_fluxes_vs_time = .true.
  write_fluxes_kxky = .false.
  write_fluxes_kxkyz = .false.
  write_moments = .false.
  write_radial_fluxes = .false.
  write_radial_moments = .false.
  autostop = .false.
/

!===============================================================================
!                              Initialise potential                             
!===============================================================================

&initialise_distribution
  initialise_distribution_option = 'default'
  phiinit = 1.0
  scale_to_phiinit = .false.
/

&initialise_distribution_maxwellian
  width0 = -3.5
  den0 = 1.
  upar0 = 0.
  even = DEPRECATED
  oddparity = .false.
  chop_side = .false.
  left = .true.
/

&initialise_distribution_noise
  zf_init = 1.0
  chop_side = .false.
  left = .true.
/

&initialise_distribution_kpar
  width0 = -3.5
  refac = 1.
  imfac = 0.
  den0 = 1.
  upar0 = 0.
  tpar0 = 0.
  tperp0 = 0.
  den1 = 0.
  upar1 = 0.
  tpar1 = 0.
  tperp1 = 0.
  den2 = 0.
  upar2 = 0.
  tpar2 = 0.
  tperp2 = 0.
  chop_side = .false.
  left = .true.
/

&initialise_distribution_rh
  kxmax = 1e+100
  kxmin = 0.0
  imfac = 0.0
  refac = 1.0
/

&restart_options
  tstart = 0.0
  scale = 1.0
  restart_file = '.nc'
  restart_dir = './'
  read_many = .true.
/

!===============================================================================
!                                  Dissipation                                  
!===============================================================================

&dissipation
  include_collisions = .false.
  collisions_implicit = .true.
  collision_model = 'dougherty'
  hyper_dissipation = .false.
/

&collisions_dougherty
  momentum_conservation = .true.
  energy_conservation = .true.
  vpa_operator = .true.
  mu_operator = .true.
/

&collisions_fp
  testpart = .true.
  fieldpart = .false.
  lmax = 1
  jmax = 1
  nvel_local = 512
  interspec = .true.
  intraspec = .true.
  iiknob = 1.0
  ieknob = 1.0
  eeknob = 1.0
  eiknob = 1.0
  eiediffknob = 1.0
  eideflknob = 1.0
  deflknob = 1.0
  eimassr_approx = .false.
  advfield_coll = .true.
  spitzer_problem = .false.
  density_conservation = .false.
  density_conservation_field = .false.
  density_conservation_tp = .false.
  exact_conservation = .false.
  exact_conservation_tp = .false.
  vpa_operator = .true.
  mu_operator = .true.
  cfac = 1
  cfac2 = 1
  nuxfac = 1
  i1fac = 1
  i2fac = 0
  no_j1l1 = .true.
  no_j1l2 = .false.
  no_j0l2 = .false.
/

&hyper
  d_hyper = 0.05
  d_zed = 0.05
  d_vpa = 0.05
  hyp_zed = .false.
  hyp_vpa = .false.
  use_physical_ksqr = .true.
  scale_to_outboard = .false.
/

