&parameters_numerical
  explicit_option = 'rk3'
  xdriftknob = CORRESPONDS TO parameters_physics:xdriftknob
  ydriftknob = CORRESPONDS TO parameters_physics:ydriftknob
  wstarknob = CORRESPONDS TO parameters_physics:wstarknob
  flip_flop = .false.
/

