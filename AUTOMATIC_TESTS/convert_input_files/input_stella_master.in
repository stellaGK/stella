!                                    Numerics                                   
!===============================================================================

! Define parameters related to numerics
&parameters_numerical
  delt = 0.03
  nstep = -1
  tend = -1.0
  delt_option = 'default'
  delt_adjust = DEPRECATED
  delt_max = -1
  delt_min = 1.e-10
  cfl_cushion = DEPRECATED
  cfl_cushion_upper = 0.5
  cfl_cushion_middle = 0.25
  cfl_cushion_lower = 0.00001
  lu_option = 'default'
  avail_cpu_time = 1.e10
  autostop = .true. 
  fphi = 1.0
  fapar = DEPRECATED
  fbpar = DEPRECATED
  fields_kxkyz = .false.
  stream_implicit = .true.
  mirror_implicit = .true.
  drifts_implicit = .false.
  driftkinetic_implicit = DEPRECATED
  stream_iterative_implicit = .false.
  stream_matrix_inversion = .false.
  mirror_semi_lagrange = .true.
  mirror_linear_interp = .false.
  use_deltaphi_for_response_matrix = .false.
  maxwellian_inside_zed_derivative = .false.
  maxwellian_normalization = .false.
  split_parallel_dynamics = .true.
  zed_upwind = 0.02
  vpa_upwind = 0.02
  time_upwind = 0.02
